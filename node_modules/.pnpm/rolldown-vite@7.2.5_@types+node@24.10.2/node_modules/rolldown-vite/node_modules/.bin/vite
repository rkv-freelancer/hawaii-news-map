#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules/rolldown-vite/bin/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules/rolldown-vite/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules/rolldown-vite/bin/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules/rolldown-vite/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/rolldown-vite@7.2.5_@types+node@24.10.2/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
