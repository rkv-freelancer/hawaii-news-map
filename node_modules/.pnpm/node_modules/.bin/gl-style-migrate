#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/maplibre-gl-style-spec/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/maplibre-gl-style-spec/dist/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/maplibre-gl-style-spec/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules/@maplibre/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/@maplibre+maplibre-gl-style-spec@23.3.0/node_modules:/Users/rkvisaya/Documents/Software Portfolios/Python Projects/Personal Projects/news_on_map--personal_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@maplibre/maplibre-gl-style-spec/dist/gl-style-migrate.mjs" "$@"
else
  exec node  "$basedir/../@maplibre/maplibre-gl-style-spec/dist/gl-style-migrate.mjs" "$@"
fi
